<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentAccountsDev">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="0dp">

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/materialCardView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            app:strokeWidth="0dp"
            app:cardElevation="2dp"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="12dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="8dp">

                <com.google.android.material.progressindicator.CircularProgressIndicator
                    android:id="@+id/circularProgressIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="28dp"
                    app:layout_constraintEnd_toStartOf="@+id/circularProgressIndicator0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/registeredUserTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicator"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicator"
                    app:layout_constraintTop_toBottomOf="@+id/circularProgressIndicator" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="Users"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicator"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicator"
                    app:layout_constraintTop_toBottomOf="@+id/registeredUserTxt" />


                <com.google.android.material.progressindicator.CircularProgressIndicator
                    android:id="@+id/circularProgressIndicator0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="28dp"
                    app:layout_constraintStart_toEndOf="@id/circularProgressIndicator"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/registeredFacultyTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicator0"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicator0"
                    app:layout_constraintTop_toBottomOf="@+id/circularProgressIndicator0" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="Faculties"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicator0"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicator0"
                    app:layout_constraintTop_toBottomOf="@+id/registeredFacultyTxt" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/materialCardView31"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            app:cardElevation="2dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialCardView21"
            app:strokeWidth="0dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="16dp">

                <TextView
                    android:id="@+id/maleCount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/textView30"
                    app:layout_constraintStart_toStartOf="@+id/textView30" />

                <TextView
                    android:id="@+id/textView30"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="Male"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toStartOf="@+id/textView31"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/maleCount" />

                <TextView
                    android:id="@+id/femaleCount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/textView31"
                    app:layout_constraintStart_toStartOf="@+id/textView31" />

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="Female"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/textView30"
                    app:layout_constraintTop_toBottomOf="@+id/maleCount" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>


        <com.google.android.material.card.MaterialCardView
            android:id="@+id/materialCardView22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            app:cardElevation="2dp"
            app:layout_constraintTop_toBottomOf="@+id/materialCardView31"
            app:strokeWidth="0dp"
            tools:layout_editor_absoluteX="12dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="8dp">

                <com.google.android.material.progressindicator.CircularProgressIndicator
                    android:id="@+id/circularProgressIndicatorA"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="28dp"
                    app:layout_constraintEnd_toStartOf="@+id/circularProgressIndicatorB"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/registeredUserTxtA"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicatorA"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicatorA"
                    app:layout_constraintTop_toBottomOf="@+id/circularProgressIndicatorA" />

                <TextView
                    android:id="@+id/textView22A"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="A"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicatorA"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicatorA"
                    app:layout_constraintTop_toBottomOf="@+id/registeredUserTxtA" />


                <com.google.android.material.progressindicator.CircularProgressIndicator
                    android:id="@+id/circularProgressIndicatorB"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="28dp"
                    app:layout_constraintEnd_toStartOf="@id/circularProgressIndicatorC"
                    app:layout_constraintStart_toEndOf="@id/circularProgressIndicatorA"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/registeredUserTxtB"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicatorB"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicatorB"
                    app:layout_constraintTop_toBottomOf="@+id/circularProgressIndicatorB" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="B"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicatorB"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicatorB"
                    app:layout_constraintTop_toBottomOf="@+id/registeredUserTxtB" />



                <com.google.android.material.progressindicator.CircularProgressIndicator
                    android:id="@+id/circularProgressIndicatorC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="28dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/circularProgressIndicatorB"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/registeredUserTxtC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:text="..."
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicatorC"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicatorC"
                    app:layout_constraintTop_toBottomOf="@+id/circularProgressIndicatorC" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="C"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/circularProgressIndicatorC"
                    app:layout_constraintStart_toStartOf="@+id/circularProgressIndicatorC"
                    app:layout_constraintTop_toBottomOf="@+id/registeredUserTxtC" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialCardView22" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>